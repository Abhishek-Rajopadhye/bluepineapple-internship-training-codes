{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-charset-utf-8": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 131",
    "not chrome <= 132",
    "not edge <= 131",
    "not firefox <= 134",
    "not safari <= 18.2",
    "not samsung <= 27",
    "not ios_saf <= 15.8",
    "not and_ff <= 132"
  ]
}